%h1.text-center
  %i.icon-media-entry
  Media Entries

.row
  = "Number of media entries:"
  %span#counter #{@media_entries.total_count}

%table.table.table-striped
  %thead
    %tr
      %th Owner
      %th Media File
      %th Id
      %th Created at
  %tbody
    =render @media_entries

= paginate @media_entries, theme: 'app_admin'
